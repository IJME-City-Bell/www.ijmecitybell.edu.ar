                            <script>
document.addEventListener("DOMContentLoaded", function () {
    // Función para detectar si es mobile
    function isMobile() {
        return window.innerWidth <= 768;
    }

    if (isMobile()) {
        // Selecciona todos los elementos con efecto atropos
        const atroposElements = document.querySelectorAll('[data-atropos]');

        atroposElements.forEach(el => {
            el.removeAttribute('data-atropos');
            el.classList.remove('atropos', 'atropos-rotate-touch');
        });
    }
});
</script>
    
