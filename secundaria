                                <script>
document.addEventListener("DOMContentLoaded", function () {
    // Detectar si es un dispositivo móvil
    function isMobile() {
        return window.innerWidth <= 768;
    }

    if (isMobile()) {
        // Selecciona todos los elementos con data-atropos
        const atroposElements = document.querySelectorAll('[data-atropos]');

        atroposElements.forEach(el => {
            el.removeAttribute('data-atropos');
            el.classList.remove('atropos', 'atropos-rotate-touch');

            // También eliminamos las capas internas para evitar que sigan afectando el DOM
            const inner = el.querySelector('.atropos-inner');
            const rotate = el.querySelector('.atropos-rotate');
            const scale = el.querySelector('.atropos-scale');
            const highlight = el.querySelector('.atropos-highlight');
            const shadow = el.querySelector('.atropos-shadow');

            if (highlight) highlight.remove();
            if (shadow) shadow.remove();

            // Reemplazamos la estructura por el contenido real
            if (inner && rotate && scale) {
                el.innerHTML = inner.innerHTML;
            }
        });
    }
});
</script>
<script>
document.addEventListener("DOMContentLoaded", function () {
  const grid = document.querySelector(".gallery-wrapper");

  if (grid) {
    const items = grid.querySelectorAll(".grid-item");
    let maxBottom = 0;

    items.forEach(item => {
      const itemRect = item.getBoundingClientRect();
      const gridRect = grid.getBoundingClientRect();
      const bottom = itemRect.top + item.offsetHeight - gridRect.top;
      if (bottom > maxBottom) maxBottom = bottom;
    });

    grid.style.height = `${maxBottom}px`;
  }
});
</script>


                            </li>
                            <!-- end gallery item -->
                        </ul>
                    </div>
                </div>
            </div>
